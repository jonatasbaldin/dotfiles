#!/bin/bash

# exports pythonpath (for testing)
# just python 3.4

export PYTHONPATH=/usr/lib/python3.4:/usr/lib/python3.4/plat-x86_64-linux-gnu:/usr/lib/python3.4/lib-dynload:/usr/local/lib/python3.4/dist-packages:/usr/lib/python3/dist-packages

for i in $@ ; do
    export PYTHONPATH=$PYTHONPATH:$i
done

